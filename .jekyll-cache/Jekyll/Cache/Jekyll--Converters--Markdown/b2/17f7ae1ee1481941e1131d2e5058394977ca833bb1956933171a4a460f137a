  xaxis: {
    title: "매매가격\n(만원)",
    range: [ -1000, 180000 ]
  },
  yaxis: {
    title: "가격 변동률\n(%)",
    range: [-30, 110]
  },
  title:'Price vs. Price Change'
    
};

